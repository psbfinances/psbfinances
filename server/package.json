{
  "name": "@psbfinances/server",
  "version": "0.1.0",
  "description": "psbfinances web",
  "homepage": "https://psbfinances.com",
  "main": "app.js",
  "type": "module",
  "types": "../typeDef/*.d.ts",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "NODE_ENV=prod PORT=443 node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/psbfinances/psbfinances.git",
    "directory": "server"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {},
    "verbose": true,
    "testRegex": "(/tests/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(validator/es/lib)/)"
    ],
    "globals": {
      "window": {}
    },
    "clearMocks": true
  },
  "devDependencies": {
    "@jest/globals": "^29.6.1",
    "@types/express": "^4.17.17",
    "jest": "^29.6.1",
    "node-mocks-http": "^1.12.2"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "csv-parse": "^5.4.0",
    "cuid": "^2.1.8",
    "errorhandler": "^1.5.1",
    "event-stream": "^4.0.1",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "express-fileupload": "^1.4.0",
    "express-rate-limit": "^6.7.1",
    "express-winston": "^4.2.0",
    "fuzzball": "^2.1.2",
    "helmet": "^7.0.0",
    "JSONStream": "^1.3.5",
    "jsonwebtoken": "^9.0.1",
    "knex": "^2.5.1",
    "mock-knex": "^0.4.12",
    "mysql": "^2.18.1",
    "node-cache": "^5.1.2",
    "object-hash": "^3.0.0",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yamljs": "^0.3.0"
